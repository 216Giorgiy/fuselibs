<Extensions Backend="CIL" Condition="USE_V8">
	<Require Condition="HOST_OSX" UnmanagedLibrary="lib/OSX/libV8Simple.dylib" />
	<Require Condition="HOST_OSX" Assembly="lib/OSX/V8Simple.net.dll" />
	<Require Condition="HOST_WIN32" Assembly="lib/DotNet/V8Simple.net.dll" />
	<Require Condition="HOST_WIN32 && HOST_X86" UnmanagedLibrary="lib/Windows/x86/V8Simple.dll" />
	<Require Condition="HOST_WIN32 && HOST_X64" UnmanagedLibrary="lib/Windows/x64/V8Simple.dll" />
</Extensions>
